<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="shortcut icon" href="{{ asset('favicon.ico') }}">

    <title>{% block title %}INTA{% endblock %}</title>
    <link href="{{ asset('assets/spotter/fonts/font-awesome.css') }}" rel="stylesheet" type="text/css">
    <link href='http://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="{{ asset('assets/spotter/bootstrap/css/bootstrap.css') }}" type="text/css">
    <link rel="stylesheet" href="{{ asset('assets/spotter/css/bootstrap-select.min.css') }}" type="text/css">
    <link rel="stylesheet" href="{{ asset('assets/spotter/css/owl.carousel.css') }}" type="text/css">
    <link rel="stylesheet" href="{{ asset('assets/spotter/css/jquery.mCustomScrollbar.css') }}" type="text/css">
    <link rel="stylesheet" href="{{ asset('assets/spotter/css/style.css') }}" type="text/css">
    <link rel="stylesheet" href="{{ asset('assets/spotter/css/user.style.css')}}" type="text/css">
    {% block extra_css %}{% endblock %}

</head>

{% block body %}
    <body onunload="" class="map-fullscreen page-homepage navigation-off-canvas" id="page-top">
        {% include 'homepage/messages/success.html.twig' %}
        {% include 'homepage/messages/danger.html.twig' %}
        {% include ':homepage/modals:report.html.twig' %}
        <!-- Outer Wrapper-->
        <div id="outer-wrapper">
            <!-- Inner Wrapper -->
            <div id="inner-wrapper">
                <!-- Navigation-->
                <div class="header">
                    <div class="wrapper">
                        <div class="brand">
                            <a href="{{ path('homepage') }}"><img src="{{ asset('assets/spotter/img/logo.png') }}" alt="logo"></a>
                        </div>
                    </div>
                </div>
                <!-- end Navigation-->
                <!-- Page Canvas-->
                <div id="page-canvas">
                    <!-- Breadcrumb -->
                    {% block search_content %}{% endblock %}
                    {% block breadcrumb_content %}{% endblock %}

                    <!--Page Content-->
                    <div id="page-content">
                        {% block content %}{% endblock %}
                    </div>
                    <!-- end Page Content-->
                </div>
                <!-- end Page Canvas-->
            </div>
            <!-- end Inner Wrapper -->
        </div>
        <!-- end Outer Wrapper-->

        <!-- Bootstrap and necessary plugins -->
        <script type="text/javascript" src="{{ asset('assets/spotter/js/jquery-2.1.0.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('assets/spotter/js/before.load.js')}}"></script>
        <script type="text/javascript" src="{{ asset('assets/spotter/js/jquery-migrate-1.2.1.min.js')}}"></script>
        <script type="text/javascript" src="{{ asset('assets/spotter/bootstrap/js/bootstrap.min.js')}}"></script>
        <script type="text/javascript" src="{{ asset('assets/spotter/js/smoothscroll.js')}}"></script>
        <script type="text/javascript" src="{{ asset('assets/spotter/js/bootstrap-select.min.js')}}"></script>
        <script type="text/javascript" src="{{ asset('assets/spotter/js/jquery.hotkeys.js')}}"></script>
        <script type="text/javascript" src="{{ asset('assets/spotter/js/jquery.nouislider.all.min.js')}}"></script>
        <script type="text/javascript" src="{{ asset('assets/spotter/js/jquery.mCustomScrollbar.concat.min.js')}}"></script>
        {#<script type="text/javascript" src="http://maps.google.com/maps/api/js?libraries=places"></script>#}
        <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCt1dtSkxqSIbtWoF-DfeUTZb906mihfh4&libraries=places"></script>
        <script type="text/javascript" src="{{ asset('assets/spotter/js/infobox.js')}}"></script>
        <script type="text/javascript" src="{{ asset('assets/spotter/js/richmarker-compiled.js')}}"></script>
        <script type="text/javascript" src="{{ asset('assets/spotter/js/markerclusterer.js')}}"></script>
        <script type="text/javascript" src="{{ asset('assets/spotter/js/custom.js')}}"></script>
        <script type="text/javascript" src="{{ asset('assets/spotter/js/maps.js')}}"></script>
        <script>
            $(window).load(function(){
                var rtl = false;
                initializeOwl(rtl);
            });
        </script>
        {% block extra_js %}{% endblock %}
    </body>
{% endblock %}
</html>

